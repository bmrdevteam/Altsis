{
  "source": {
    "include": ["."],
    "includePattern": ".(jsx|js|ts|tsx)$",
    "exclude": ["node_modules", "docs"]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    },
    "docdash": {
      "static": false
    }
  }
}
